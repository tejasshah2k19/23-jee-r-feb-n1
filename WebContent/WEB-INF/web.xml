		<servlet-class>com.controller.EcomListUserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EcomListUserServlet</servlet-name>
		<url-pattern>/EcomListUserServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>DeleteUserServlet</display-name>
		<servlet-name>DeleteUserServlet</servlet-name>
