	</servlet-mapping>

	<servlet>
		<description></description>
		<display-name>ALoginServlet</display-name>
		<servlet-name>ALoginServlet</servlet-name>
		<servlet-class>com.controller.ALoginServlet</servlet-class>
		<init-param>
			<param-name>email</param-name>
			<param-value>admin@gmail.com</param-value>
		</init-param>

		<init-param>
			<param-name>password</param-name>
			<param-value>admin123</param-value>
		</init-param>

	</servlet>


	<servlet-mapping>
		<servlet-name>ALoginServlet</servlet-name>
		<url-pattern>/ALoginServlet</url-pattern>
	</servlet-mapping>

	<context-param>
		<param-name>appName</param-name>
		<param-value>AdvJava</param-value>
	</context-param>

	<jsp-config>
		<taglib>
			<taglib-uri>www.royal.com/stag</taglib-uri>
			<taglib-location>/WEB-INF/stag.tld</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>www.royal.com/ctag</taglib-uri>
			<taglib-location>/WEB-INF/ctag.tld</taglib-location>
		</taglib>

	</jsp-config>
</web-app>